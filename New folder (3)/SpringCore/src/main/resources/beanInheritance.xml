<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd	
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    
    <bean class = "com.beaninheritance.Employee"   id=" emp">
    <property  name = "id"   value ="11"></property>
    <property name= "name"   value = " nitha"></property>
     <property  name = "gender"   value =" F"></property>
    </bean>
    
    <bean class = "com.beaninheritance.RegEmployee"  id = "regEmp"  parent = "emp">
     <property  name = "salary"   value ="9999"></property>
      <property  name = "bonus"   value ="99"></property>
       </bean>
    
    
</beans>